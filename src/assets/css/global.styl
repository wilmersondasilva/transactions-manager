@import url('https://fonts.googleapis.com/css?family=Noto+Sans&display=swap')

*
    margin 0
    padding 0
    border 0

html, body
    width 100%
    height 100%

body
    background-color #f8f8f8
    color #2c3e50
    font-family 'Noto Sans', sans-serif
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
